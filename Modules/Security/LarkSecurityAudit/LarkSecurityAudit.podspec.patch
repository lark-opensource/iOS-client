def security_subspecs
  if ENV['BUILD_PRODUCT_TYPE'] == 'ASSEMBLY'
    puts 'enable Assembly version'.red
    {subspecs: ['Core', 'Assembly', 'Authorization']}
  else
    puts 'use Core version'.green
    {subspecs: ['Core', 'Assembly', 'Authorization']}
  end
end

debug_dependency 'LarkSecurityAudit', **security_subspecs, :integration => :local
