{
  "name": "DebugRouter",
  "version": "2.1.5",
  "summary": "framework_summary",
  "homepage": "https://code.byted.org/lynx/DebugRouter",
  "license": "MIT",
  "authors": "Lynx",
  "source": {
    "git": "git@code.byted.org:lynx/DebugRouter.git",
    "commit": "2cf575f9f5f2016a6a953183e57c4c0168f6d723"
  },
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "OS_IOS=1 DebugRouter_POD_VERSION=@\\\"9999_2.1.5\\\"",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "platforms": {
    "ios": "9.0"
  },
  "subspecs": [
    {
      "name": "Framework",
      "source_files": "iOS/DebugRouter/DebugRouter/Classes/**/*",
      "exclude_files": "iOS/DebugRouter/DebugRouter/Classes/net/macOS/*",
      "pod_target_xcconfig": {
        "HEADER_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}/iOS/DebugRouter/DebugRouter/Classes\" \"${PODS_TARGET_SRCROOT}/Native\" \"${PODS_TARGET_SRCROOT}/ThirdParty/jsoncpp/include\""
      },
      "dependencies": {
        "SocketRocket": []
      },
      "public_header_files": "iOS/DebugRouter/DebugRouter/Classes/**/*.{h}"
    },
    {
      "name": "Native",
      "header_mappings_dir": "Native",
      "source_files": "Native/**/*",
      "exclude_files": [
        "Native/net/**/*",
        "Native/test/*",
        "Native/socket/*"
      ],
      "pod_target_xcconfig": {
        "HEADER_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}/Native/\" \"${PODS_TARGET_SRCROOT}/ThirdParty/jsoncpp/include\""
      },
      "dependencies": {
        "DebugRouter/ThirdParty": []
      },
      "private_header_files": "Native/**/*.{h}",
      "public_header_files": "Native/protocol/protocol_message.h"
    },
    {
      "name": "ThirdParty",
      "libraries": "stdc++",
      "pod_target_xcconfig": {
        "HEADER_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}/Native/\" \"${PODS_TARGET_SRCROOT}/ThirdParty/jsoncpp/include\""
      },
      "subspecs": [
        {
          "name": "jsoncpp",
          "header_mappings_dir": "ThirdParty",
          "source_files": [
            "ThirdParty/jsoncpp/include/json/*.h",
            "ThirdParty/jsoncpp/src/lib_json/json_tool.h",
            "ThirdParty/jsoncpp/src/lib_json/json_reader.cpp",
            "ThirdParty/jsoncpp/src/lib_json/json_value.cpp",
            "ThirdParty/jsoncpp/src/lib_json/json_writer.cpp",
            "ThirdParty/jsoncpp/src/lib_json/json_valueiterator.inl"
          ],
          "compiler_flags": [
            "-Wno-documentation",
            "-Wno-deprecated"
          ],
          "private_header_files": [
            "ThirdParty/jsoncpp/**/*.{h}",
            "ThirdParty/jsoncpp/**/*.{inl}"
          ]
        }
      ]
    }
  ],
  "_bytebus_platform": {
    "repo_id": "16385",
    "app_id": "9999"
  },
  "repoId": "16385",
  "description": "framework_description",
  "user_target_xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "GCC_PREPROCESSOR_DEFINITIONS": "DebugRouter_POD_VERSION=@\\\"9999_2.1.5\\\""
  }
}